{
	"name": "@omnygroup/eslint",
	"version": "1.0.0",
	"description": "Shared ESLint configuration for Omny projects with TypeScript, SonarJS, and Prettier support",
	"type": "module",
	"main": "./index.js",
	"exports": {
		".": "./index.js",
		"./base": "./configs/base.js",
		"./typescript": "./configs/typescript.js",
		"./import": "./configs/import.js",
		"./sonar": "./configs/sonar.js",
		"./prettier": "./prettier/index.js",
		"./all": "./all.js"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"keywords": [
		"eslint",
		"eslintconfig",
		"typescript",
		"sonarjs",
		"prettier",
		"code-quality",
		"linting"
	],
	"author": "Omny",
	"license": "MIT",
	"peerDependencies": {
		"eslint": "9.39.2",
		"typescript": "5.9.3"
	},
	"dependencies": {
		"@eslint/js": "9.39.2",
		"eslint-config-prettier": "10.1.8",
		"eslint-import-resolver-typescript": "4.4.4",
		"eslint-plugin-import": "2.32.0",
		"eslint-plugin-sonarjs": "3.0.5",
		"typescript-eslint": "8.52.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/omnygroup/eslint.git"
	},
	"bugs": {
		"url": "https://github.com/omnygroup/eslint/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"index.js",
		"all.js",
		"configs/",
		"prettier/",
		"templates/",
		"README.md",
		"LICENSE",
		"CHANGELOG.md"
	],
	"scripts": {
		"test": "echo \"No tests\" && exit 0",
		"release": "semantic-release"
	},
	"devDependencies": {
		"@semantic-release/changelog": "6.0.3",
		"@semantic-release/commit-analyzer": "13.0.1",
		"@semantic-release/github": "12.0.2",
		"@semantic-release/npm": "13.1.3",
		"@semantic-release/release-notes-generator": "14.1.0",
		"semantic-release": "25.0.2"
	}
}
